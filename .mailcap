# MS Word documents
application/msword; open %s;
application/octet-stream; open %s;
application/vnd.openxmlformats-officedocument.wordprocessingml.document; open %s;

# Images
image/jpeg; open %s
image/pjpeg; open %s
image/png; open %s
image/gif; open %s

# PDF
application/pdf;                open %s;

# HTML
text/html;                      open %s; nametemplate=%s.html

# Text
text/plain;                     nvim %s; nametemplate=%s.text;
text/csv;                       nvim %s; nametemplate=%s.text;
